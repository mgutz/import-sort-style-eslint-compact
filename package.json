{
  "name": "import-sort-style-eslint-compact",
  "version": "2.4.0",
  "description": "An import-sort style that is compatible with ESLint's sort-imports rule",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc -p src",
    "watch:build": "tsc -p src -w",
    "test": "mocha --compilers ts:ts-node/register,tsx:ts-node/register --recursive test",
    "watch:test": "mocha -w --compilers ts:ts-node/register,tsx:ts-node/register --recursive test"
  },
  "author": "Mario Gutierrez <mario@mgutz.com> (https://github.com/mgutz)",
  "repository": "mgutz/import-style-eslint-compact",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^3.4.32",
    "@types/mocha": "^2.2.31",
    "@types/node": "^6.0.38",
    "chai": "^3.5.0",
    "import-sort-parser": "^2.0.14",
    "import-sort-style": "^2.3.0",
    "mocha": "^3.0.2",
    "ts-node": "^1.3.0",
    "tslint": "^3.15.1",
    "typescript": "^2.1.4"
  },
  "dependencies": {}
}
